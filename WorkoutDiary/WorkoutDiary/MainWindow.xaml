<Window x:Class="WorkoutDiary.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkoutDiary"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnSignIn" Content="Sign In" HorizontalAlignment="Center" Height="60" Width="250" Margin="0, 0, 0, 100" Click="btnSignIn_Click"/>
        <Button x:Name="btnRegister" Content="Register" HorizontalAlignment="Center" Height="60" Width="250" Margin="134,165,133,94" Click="btnRegister_Click"/>
    </Grid>
</Window>
